---
import BaseHead from "../components/BaseHead.astro";

export interface Props {
    content: {
        title: string,
        description: string
    }
}

const { content: { title, description } } = Astro.props as Props

---

<html lang="en">

<head>
    <BaseHead title={title} description={description}></BaseHead>
</head>

<body>
    <slot />
</body>

</html>